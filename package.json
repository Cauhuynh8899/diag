{
  "name": "diag",
  "version": "0.0.0",
  "private": "true",
  "description": "Diagnostic network issues while connecting to Algolia",
  "main": "index.js",
  "scripts": {
    "build": "webpack && ./scripts/build.js",
    "serve-build": "browser-sync start --server public/ --no-open",
    "test": "npm run lint",
    "webpack-watch": "webpack -d --watch --colors --progress",
    "browser-sync": "browser-sync start --server frontend/ --files 'frontend/*' --no-open",
    "dev": "./scripts/dev.sh",
    "lint": "eslint --quiet .",
    "publish": "npm run build && ./scripts/publish.js"
  },
  "repository": "algolia/diag",
  "keywords": [
    "algolia",
    "diagnostic"
  ],
  "dependencies": {
    "algoliasearch": "3.6.0",
    "async": "1.2.1",
    "humanize-number": "0.0.2",
    "lodash": "3.9.3",
    "queue": "3.1.0",
    "superagent": "1.2.0",
    "ua-parser-js": "0.7.7"
  },
  "devDependencies": {
    "browser-sync": "2.7.10",
    "bundle-collapser": "1.2.0",
    "css-loader": "0.14.5",
    "eslint": "0.23.0",
    "gulp": "3.9.0",
    "gulp-filter": "2.0.2",
    "gulp-gh-pages": "0.5.2",
    "gulp-if": "1.2.5",
    "gulp-minify-html": "1.0.3",
    "gulp-rev": "4.0.0",
    "gulp-rev-replace": "0.4.2",
    "gulp-uglify": "1.2.0",
    "gulp-useref": "1.2.0",
    "json": "9.0.3",
    "mversion": "1.10.0",
    "pretty-bytes": "2.0.1",
    "semver": "4.3.6",
    "style-loader": "0.12.3",
    "webpack": "1.9.11",
    "webpack-dev-server": "1.9.0"
  },
  "homepage": "https://github.com/algolia/diag",
  "bugs": "https://github.com/algolia/diag/issues",
  "author": "Algolia SAS <support@algolia.com> (https://www.algolia.com)",
  "engines": {
    "node": ">=0.8"
  },
  "license": "MIT"
}
